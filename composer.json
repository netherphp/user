{
	"name": "netherphp/user",
	"description": "Provides a basic user login system.",
	"keywords": [ "php", "user" ],
	"license": "BSD-2-Clause",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors":[
		{
			"name": "Bob Majdak Jr",
			"email": "bmajdak@php.net",
			"homepage": "http://pegasusgate.net/+bob",
			"role": "developer"
		}
	],
	"config": {
		"preferred-install": {
			"netherphp/*": "source",
			"*": "dist"
		},
		"allow-plugins": {
			"netherphp/composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php": "^8.1",
		"netherphp/database": "dev-redux",
		"netherphp/common": "dev-master",
		"league/oauth2-github": "^3.1",
		"league/oauth2-google": "^4.0",
		"patrickbussmann/oauth2-apple": "^0.2.10",
		"wohali/oauth2-discord-new": "^1.2"
	},
	"require-dev": {
		"netherphp/standards": ">=0.1.9",
		"phpunit/phpunit": "^9.5",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
	},
	"bin": [
		"bin/nuser"
	],
	"autoload": {
		"psr-0": {
			"Nether\\":"src/"
		}
	}
}
